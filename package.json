{
  "name": "monday-funnel-widget",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node -e \"require('zip-a-folder').zip('./build', './build.zip').then(() => console.log('Compressed ./build directory to ./build.zip'))\"",
    "test": "react-scripts test --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.js \"{src, test}/**/*.{js,ts,tsx}\" --fix",
    "expose": "ngrok http 8301",
    "release": "standard-version",
    "prepare": "husky install",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "engines": {
    "node": ">=18"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.1",
    "monday-sdk-js": "^0.1",
    "monday-ui-react-core": "^1.0",
    "react": "^18.0",
    "react-bootstrap": "^2.4",
    "react-dom": "^18.0",
    "react-funnel-pipeline": "^0.1",
    "react-scripts": "^5.0",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0",
    "commitlint": "^17.0",
    "eslint": "^8.6",
    "eslint-config-prettier": "^8.3",
    "eslint-plugin-prettier": "^4.0",
    "husky": "^8.0",
    "jest": "^28.1.3",
    "ngrok": "^4.0",
    "prettier": "^2.7",
    "standard-version": "^9.5",
    "zip-a-folder": "^1.1"
  }
}
